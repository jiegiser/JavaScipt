        $(function () {
            var vSql = "select 代码名称,代码 from bdc_codetbl where 代码类型='登记类型' order by 代码 asc ";
            $("#tableGrid").empty();
            var gridProp = createtableGrid();
            //BindTableGrid_Dd(1, 1);
            $(".text-border").blur(function () {
                var name = $(this).attr("name");
                $("[name=" + name + "]").val($(this).val());
            });
            getData();
            $("select[name='SelectState']").change(function () {
                var i = $(this).children('option:selected').val();
                $("select[name='SelectState']").val(i);
            });
        });
        //创建表格
        function createtableGrid() {
            var vStrSql = "";
            gridProp = null;
            gridProp = new tableGrid("gridProp");
            gridProp.ID = "tableGrid";
            gridProp.FilterString = " 1>0 ";
            gridProp.PageSize = 10; //设置页面显示行数
            gridProp.OrderColumn = " 房号 "; //设置排序字段
            gridProp.OrderType = " ASC ";
            gridProp.IsPage = true; //是否分页
            gridProp.Edit = false; //支持编辑
            gridProp.initTable = function () {
                //根据对应点击的信息进行选择查询
                var selectedQLLX = $(".selected").attr("name");
                BindTableGrid(selectedQLLX, 'selectPage');
            };
        }
        var temp = 1;
        //循环查询所有的权利，定位到有数据的权利标签上
        function BindTableGrid(vQLLX, selectpage) {
            //拼接查询条件
            // 查询条件为空默认可以查询
            gridProp.FilterString = " 1>0 ";
            var vFilterString = GetFilterString(vQLLX);
            if (!CheckObjectIsNull(vFilterString)) {
                gridProp.FilterString += vFilterString;
            }
            var vModel = gridProp.getModel();
            if (selectpage == "7") {
                vModel.SelectPage = 7;
            }
            showMask();
            $.post("GetCompositeQueryInfo?sQLLX=" + vQLLX, vModel, function (response) {
                //$(".btnLineDl dd").find("a[name='" + vQLLX + "']").addClass("selected");
                if (vQLLX == "1") {
                    //显示不动产权信息
                    Showtb_bdc();
                } else if (vQLLX == "2") {
                    //显示抵押权信息
                    Showtb_dy();
                } else if (vQLLX == "3") {
                    //显示查封信息
                    Showtb_cf();
                } else if (vQLLX == "4") {
                    //显示预告信息
                    Showtb_yg();
                } else if (vQLLX == "5") {
                    //显示异议信息
                    Showtb_yy();
                } else if (vQLLX == "6") {
                    //显示地役权信息
                    Showtb_dyq();
                }
                else if (vQLLX == "7") {//增加注销 赵风 2016年10月19日
                    //显示地役权信息
                    Showtb_zx();
                }

                //循环显示，定位到到有数据的权利标签上
                var iQLLX = parseInt(vQLLX);
                if (!CheckObjectIsNull(response)) {
                    gridProp.initGrid(JSON.parse(response));
                    temp = 1;
                    ShowQuery(iQLLX);
                    if (vQLLX != vTable)
                        $(".btnLineDl dd").find("a[name='" + vTable + "']").click();
                }
                else {
                    if (!checkFilter() && temp < 7) {
                        temp += 1;
                        BindTableGrid((iQLLX + 1) % 6, 1);
                    }
                    else {
                        gridProp.initGrid(JSON.parse("{\"DataCount\":\"0\",\"DataList\":\"\"}"));
                        temp = 1;
                        ShowQuery(iQLLX);
                    }
                }
                hideMask();

                var vTotelHight = $("td[class='grid-cell']").css("height");
                var vHeight = (parseFloat(vTotelHight) - 12) / 2;
                $("#tableGrid .temporary_Sign").css("margin-top", vHeight + "px");
                $("#tableGrid .valid_Sign").css("margin-top", vHeight + "px");
                $("#tableGrid .history_Sign").css("margin-top", vHeight + "px");
                $("#tableGrid .unvalid_Sign").css("margin-top", vHeight + "px");
            }, 'json');
        }
        function canView(obj) {
            if (obj.indexOf("历史案卷录入") > -1) {
                return true;
            }
            else {
                return false;
            }
        }
        //格式化日期
        function ConvertDate(vDate) {
            if (vDate) {
                return vDate.replace(" 0:00:00", "");
            }
            return "";
        }
        function CaseNoState(vCaseNo) {
            var vStateSign = vCaseNo;
            if (vStateSign == "0") {
                vState = "<div style='margin:0 auto;width:100%;height:100%;text-align:center;' valign='middle'><div class='temporary_Sign' style='margin:0 auto; margin-top: 5px;'></div></div>";
            }
            else if (vStateSign == "1") {
                vState = "<div style='margin:0 auto;width:100%;height:100%;text-align:center;' valign='middle'><div class='valid_Sign' style='margin:0 auto;'></div></div>";
            }
            else if (vStateSign == "2" || vStateSign == "4") {
                vState = "<div style='margin:0 auto;width:100%;height:100%;text-align:center;' valign='middle'><div class='history_Sign' style='margin:0 auto;'></div></div>";
            }
            else {
                vState = "<div style='margin:0 auto;width:100%;height:100%;text-align:center;' valign='middle'><div class='unvalid_Sign' style='margin:0 auto;'></div></div>";
            }
            return vState;
        }
        function ShowQuery(QLLX) {
            $(".table-sgl.query").css("display", "none");
            switch (QLLX) {
                case 1:
                    $("#tb_bdccx").css("display", "block");
                    break;
                case 2:
                    $("#tb_dycx").css("display", "block");
                    break;
                case 3:
                    $("#tb_cfcx").css("display", "block");
                    break;
                case 4:
                    $("#tb_ygcx").css("display", "block");
                    break;
                case 5:
                    $("#tb_yycx").css("display", "block");
                    break;
                case 6:
                    $("#tb_dyqcx").css("display", "block");
                    break;
                case 7:
                    $("#tb_zxcx").css("display", "block");
                    break;
            }
        }
        function search() {
            //业务类型
            var selectedQLLX = $(".selected").attr("name");
            $("#tableGrid").empty();
            var gridProp = createtableGrid();
            BindTableGrid_Dd(selectedQLLX, 1);
        }
        var vTable = "";
        $(".btnLineDl dd").click(function () {
            $(".btnLineDl dd").find("a").removeClass("selected");
            $(this).find("a").addClass("selected");
            var selecteddd = $(".selected").attr("name");
            vTable = selecteddd;
            ShowQuery(parseInt(selecteddd));

            //清除查询条件d
            var selectedQLLX = $(".selected").attr("name");
            $("#tableGrid").empty();
            var gridProp = createtableGrid();
            //BindTableGrid_Dd(selectedQLLX, 'selectPage');
        });
        //为标签的切换，不会循环查询所有权利，只查询当前的权利
        /*
        
        */
        function BindTableGrid_Dd(vQLLX, selectpage) {
            //拼接查询条件
            gridProp.FilterString = " 1>2 ";
            // 查询条件为空默认可以查询 赵风 2016年10月24日
            gridProp.FilterString = " 1>0 and SUBSYSTEMID=@Request.QueryString["subSystemId"]";
            var vFilterString = GetFilterString(vQLLX);
            if (!CheckObjectIsNull(vFilterString)) {
                gridProp.FilterString = " 1>0 and SUBSYSTEMID=@Request.QueryString["subSystemId"] " + vFilterString;
            }

            var vModel = gridProp.getModel();
            if (selectpage == "1") {
                vModel.SelectPage = 1;
            }
            showMask();
            $(".btnLineDl dd").find("a").removeClass("selected");
            $.post("GetCompositeQueryInfo?sQLLX=" + vQLLX, vModel, function (response) {
                $(".btnLineDl dd").find("a[name='" + vQLLX + "']").addClass("selected");
                if (vQLLX == "1") {
                    //显示不动产权信息
                    Showtb_bdc();
                } else if (vQLLX == "2") {
                    //显示抵押权信息
                    Showtb_dy();
                } else if (vQLLX == "3") {
                    //显示查封信息
                    Showtb_cf();
                } else if (vQLLX == "4") {
                    //显示预告信息
                    Showtb_yg();
                } else if (vQLLX == "5") {
                    //显示异议信息
                    Showtb_yy();
                } else if (vQLLX == "6") {
                    //显示地役权信息
                    Showtb_dyq();
                } else if (vQLLX == "7") {
                    //显示注销信息
                    Showtb_zx();
                }

                //显示数据
                if (!CheckObjectIsNull(response)) {
                    gridProp.initGrid(JSON.parse(response));
                }
                else {
                    gridProp.initGrid(JSON.parse("{\"DataCount\":\"0\",\"DataList\":\"\"}"));
                }
                hideMask();

                var vTotelHight = $("td[class='grid-cell']").css("height");
                var vHeight = (parseFloat(vTotelHight) - 12) / 2;
                $("#tableGrid .temporary_Sign").css("margin-top", vHeight + "px");
                $("#tableGrid .valid_Sign").css("margin-top", vHeight + "px");
                $("#tableGrid .history_Sign").css("margin-top", vHeight + "px");
                $("#tableGrid .unvalid_Sign").css("margin-top", vHeight + "px");
            }, 'json');
        }

        //将页面上的查询条件凭借成查询字符串
        /*
        vQLLX为查询类型
        */
        function GetFilterString(vQLLX) {
            var vFilterString = "";
            //业务号
            if (!CheckObjectIsNull($("[name=txtYeWuHao]:visible").val())) {
                vFilterString += " and (业务号 like '%" + $("[name=txtYeWuHao]:visible").val().toUpperCase() + "%'" +
                " or 业务号 like '%" + $("[name=txtYeWuHao]:visible").val().toLowerCase() + "%')";
            }
            //不动产单元号
            if (!CheckObjectIsNull($("[name=txtBuDongChanDanYuanHao]:visible").val())) {
                vFilterString += " and (不动产单元号 like '%" + $("[name=txtBuDongChanDanYuanHao]:visible").val().toUpperCase() + "%'" +
                " or 不动产单元号 like '%" + $("[name=txtBuDongChanDanYuanHao]:visible").val().toLowerCase() + "%')";
            }

            ////受理编号
            if (!CheckObjectIsNull($("[name=txtSLBH]:visible").val())) {
                vFilterString += " and (受理编号 like '%" + $("[name=txtSLBH]:visible").val().toUpperCase() + "%'" +
                " or 受理编号 like '%" + $("[name=txtSLBH]:visible").val().toLowerCase() + "%')";
            }

            var selectedQLLX = $(".selected").attr("name");
            //登记类别
            if (!CheckObjectIsNull($("[name=SelectDengJiLeiBie]:visible").val())) {

                var djlbID = "";
                switch (selectedQLLX) {
                    case "1":
                        djlbID = "djlb_bdc";
                        break;
                    case "2":
                        djlbID = "djlb_dy";
                        break;
                    case "3":
                        djlbID = "djlb_cf";
                        break;
                    case "4":
                        djlbID = "djlb_yg";
                        break;
                    case "5":
                        djlbID = "djlb_yy";
                        break;
                    case "6":
                        djlbID = "djlb_dyq";
                        break;
                    case "7":
                        djlbID = "djlb_zx";
                        break;
                    default:
                        djlbID = "djlb_bdc";
                        break;
                }
                vFilterString += " and 登记类别='" + $("#" + djlbID).find("option:selected").text() + "' ";
            }
            // 不动产登记证明号
            if (selectedQLLX == "1" || selectedQLLX == "3") {
                if (!CheckObjectIsNull($("[name=txtBuDongChanQuanZhengHao]:visible").val())) {
                    vFilterString += " and  (不动产权证号 like '%" + $("[name=txtBuDongChanQuanZhengHao]:visible").val().toUpperCase() + "%'" +
                    " or 不动产权证号 like '%" + $("[name=txtBuDongChanQuanZhengHao]:visible").val().toLowerCase() + "%')";
                }
            }
            else {
                if (!CheckObjectIsNull($("[name=txtBuDongChanQuanZhengHao]:visible").val())) {
                    vFilterString += " and  (不动产登记证明号 like '%" + $("[name=txtBuDongChanQuanZhengHao]:visible").val().toUpperCase() + "%'" +
                    " or 不动产登记证明号 like '%" + $("[name=txtBuDongChanQuanZhengHao]:visible").val().toLowerCase() + "%')";
                }
            }
            ////不动产登记证明号
            //if (!CheckObjectIsNull($("[name=txtBuDongChanQuanZhengHao]:visible").val())) {
            //    vFilterString += " and  (不动产登记证明号 like '%" + $("[name=txtBuDongChanQuanZhengHao]:visible").val().toUpperCase() + "%'" +
            //    " or 不动产登记证明号 like '%" + $("[name=txtBuDongChanQuanZhengHao]:visible").val().toLowerCase() + "%'" +
            //    " or 原不动产权证号 like '%" + $("[name=txtBuDongChanQuanZhengHao]:visible").val().toUpperCase() + "%'" +
            //    " or 原不动产权证号 like '%" + $("[name=txtBuDongChanQuanZhengHao]:visible").val().toLowerCase() + "%')";
            //}
            // 坐落
            if (!CheckObjectIsNull($("[name=txt_zl]:visible").val())) {
                vFilterString += " and  (坐落 like '%" + $("[name=txt_zl]:visible").val().toUpperCase() + "%'" +
                " or 坐落 like '%" + $("[name=txt_zl]:visible").val().toLowerCase() + "%')";
            }
            //登记时间起
            if (!CheckObjectIsNull($("[name=txtDengJiRiQiQi]:visible").val())) {
                if ("@ViewBag.DBType" == "ORACLE") {
                    vFilterString += " and 登记时间>=to_date('" + $("[name=txtDengJiRiQiQi]:visible").val() + "','yyyy-mm-dd')  ";
                } else {
                    vFilterString += " and 登记时间>='" + $("[name=txtDengJiRiQiQi]:visible").val() + "'  ";
                }
            }
            //登记时间止
            if (!CheckObjectIsNull($("[name=txtDengJiRiQiZhi]:visible").val())) {
                if ("@ViewBag.DBType" == "ORACLE") {
                    vFilterString += " and 登记时间<=to_date('" + $("[name=txtDengJiRiQiZhi]:visible").val() + "','yyyy-mm-dd') ";
                } else {
                    vFilterString += " and 登记时间<='" + $("[name=txtDengJiRiQiZhi]:visible").val() + "' ";
                }
            }
            //查封时间起
            if (vQLLX == "3" && !CheckObjectIsNull($("#txt_cfsjq").val())) {
                if ("@ViewBag.DBType" == "ORACLE") {
                    vFilterString += " and 查封时间>=to_date('" + $("#txt_cfsjq").val() + "','yyyy-mm-dd')  ";
                } else {
                    vFilterString += " and 查封时间>='" + $("#txt_cfsjq").val() + "'  ";
                }
            }
            //查封时间止
            if (vQLLX == "3" && !CheckObjectIsNull($("#txt_cfsjz").val())) {
                if ("@ViewBag.DBType" == "ORACLE") {
                    vFilterString += " and 查封时间<=to_date('" + $("#txt_cfsjz").val() + "','yyyy-mm-dd')  ";
                } else {
                    vFilterString += " and 查封时间<='" + $("#txt_cfsjz").val() + "'  ";
                }
            }
            //权利起始时间起
            if (!CheckObjectIsNull($("[name=txtQLQSSJQ]:visible").val())) {
                if ("@ViewBag.DBType" == "ORACLE") {
                    vFilterString += " and 起始时间>=to_date('" + $("[name=txtQLQSSJQ]:visible").val() + "','yyyy-mm-dd')  ";
                } else {
                    vFilterString += " and 起始时间>='" + $("[name=txtQLQSSJQ]:visible").val() + "'  ";
                }
            }
            //权利起始时间止
            if (!CheckObjectIsNull($("[name=txtQLQSSJZ]:visible").val())) {
                if ("@ViewBag.DBType" == "ORACLE") {
                    vFilterString += " and 起始时间<=to_date('" + $("[name=txtQLQSSJZ]:visible").val() + "','yyyy-mm-dd') ";
                } else {
                    vFilterString += " and 起始时间<='" + $("[name=txtQLQSSJZ]:visible").val() + "' ";
                }
            }
            //权利结束时间起
            if (!CheckObjectIsNull($("[name=txtQLJSSJQ]:visible").val())) {
                if ("@ViewBag.DBType" == "ORACLE") {
                    vFilterString += " and 结束时间>=to_date('" + $("[name=txtQLJSSJQ]:visible").val() + "','yyyy-mm-dd') ";
                } else {
                    vFilterString += " and 结束时间>='" + $("[name=txtQLJSSJQ]:visible").val() + "' ";
                }
            }
            //权利结束时间止
            if (!CheckObjectIsNull($("[name=txtQLJSSJZ]:visible").val())) {
                if ("@ViewBag.DBType" == "ORACLE") {
                    vFilterString += " and 结束时间<=to_date('" + $("[name=txtQLJSSJZ]:visible").val() + "','yyyy-mm-dd') ";
                } else {
                    vFilterString += " and 结束时间<='" + $("[name=txtQLJSSJZ]:visible").val() + "' ";
                }
            }
            if (vQLLX == "1" && !CheckObjectIsNull($("#txt_qlr").val())) {
                vFilterString += " and    业务号 in (select distinct 业务号 from bdc_qlr where 权利人类别 in ('权利人','原权利人') and 权利人名称 like '%" + $("#txt_qlr").val() + "%')";
            }
            if (vQLLX == "1" && !CheckObjectIsNull($("#txt_qlrsfzh").val())) {
                vFilterString += " and  业务号 in (select distinct 业务号 from bdc_qlr where 权利人类别 in ('权利人','原权利人') and (证件号 like '%" + $("#txt_qlrsfzh").val().toUpperCase() + "%'" +
                " or 证件号 like '%" + $("#txt_qlrsfzh").val().toLowerCase() + "%' )) ";

            }
            if (vQLLX == "2" && !CheckObjectIsNull($("#txt_dyr").val())) {
                vFilterString += " and 业务号 in (select distinct 业务号 from bdc_qlr where 权利人类别 = '抵押人' and 权利人名称 like '%" + $("#txt_dyr").val() + "%')";
            }
            if (vQLLX == "2" && !CheckObjectIsNull($("#txt_dyrsfzh").val())) {
                vFilterString += " and 业务号 in (select distinct 业务号 from bdc_qlr where 权利人类别 = '抵押人'  and  (证件号 like '%" + $("#txt_dyrsfzh").val().toUpperCase() + "%'" +
                " or 证件号 like '%" + $("#txt_dyrsfzh").val().toLowerCase() + "%'))";
            }
            if (vQLLX == "2" && !CheckObjectIsNull($("#txt_dyqr").val())) {
                vFilterString += " and 业务号 in (select distinct 业务号 from bdc_qlr where 权利人类别 = '抵押权人' and 权利人名称 like '%" + $("#txt_dyqr").val() + "%')";
            }
            if (vQLLX == "4" && !CheckObjectIsNull($("#txt_ygqlr").val())) {
                vFilterString += " and 业务号 in (select distinct 业务号 from bdc_qlr where 权利人类别 = '预告权利人' and 权利人名称 like '%" + $("#txt_ygqlr").val() + "%')";
            }
            if (vQLLX == "4" && !CheckObjectIsNull($("#txt_ygqlrsfzh").val())) {
                vFilterString += " and 业务号 in (select distinct 业务号 from bdc_qlr where 权利人类别 = '预告权利人'  and  (证件号 like '%" + $("#txt_ygqlrsfzh").val().toUpperCase() + "%'" +
                " or 证件号 like '%" + $("#txt_ygqlrsfzh").val().toLowerCase() + "%'))";
            }
            if (vQLLX == "4" && !CheckObjectIsNull($("#txt_ygywr").val())) {
                vFilterString += " and 业务号 in (select distinct 业务号 from bdc_qlr where 权利人类别 = '预告义务人' and 权利人名称 like '%" + $("#txt_ygywr").val() + "%')";
            }
            if (vQLLX == "4" && !CheckObjectIsNull($("#txt_ygywrsfzh").val())) {
                vFilterString += " and 业务号 in (select distinct 业务号 from bdc_qlr where 权利人类别 = '预告义务人'  and  (证件号 like '%" + $("#txt_ygywrsfzh").val().toUpperCase() + "%'" +
                " or 证件号 like '%" + $("#txt_ygywrsfzh").val().toLowerCase() + "%'))";
            }
            if (vQLLX == "5" && !CheckObjectIsNull($("#txt_yysqr").val())) {
                vFilterString += " and 业务号 in (select distinct 业务号 from bdc_qlr where 权利人类别 = '异议申请人' and 权利人名称 like '%" + $("#txt_yysqr").val() + "%')";
            }
            if (vQLLX == "5" && !CheckObjectIsNull($("#txt_yysqrsfzh").val())) {
                vFilterString += " and 业务号 in (select distinct 业务号 from bdc_qlr where 权利人类别 = '异议申请人'  and  (证件号 like '%" + $("#txt_yysqrsfzh").val().toUpperCase() + "%'" +
                " or 证件号 like '%" + $("#txt_yysqrsfzh").val().toLowerCase() + "%'))";
            }

            //抵押权 顺位
            if (vQLLX == "2" && !CheckObjectIsNull($("#txt_dysw").val())) {
                vFilterString += " and (抵押顺位 like '%" + $("#txt_dysw").val().toUpperCase() + "%'" +
                " or 抵押顺位 like '%" + $("#txt_dysw").val().toLowerCase() + "%')";
            }
            //查封 查封文号
            if (vQLLX == "3" && !CheckObjectIsNull($("#txt_cfwh").val())) {
                vFilterString += " and (查封文号 like '%" + $("#txt_cfwh").val().toUpperCase() + "%'" +
                " or 查封文号 like '%" + $("#txt_cfwh").val().toLowerCase() + "%')";
            }
            //查封 解封业务号
            if (vQLLX == "3" && !CheckObjectIsNull($("#txt_jfywh").val())) {
                vFilterString += " and (解封业务号 like '%" + $("#txt_jfywh").val().toUpperCase() + "%'" +
                " or 解封业务号 like '%" + $("#txt_jfywh").val().toLowerCase() + "%')";
            }
            //查封 解封文号
            if (vQLLX == "3" && !CheckObjectIsNull($("#txt_jfwh").val())) {
                vFilterString += " and (解封文号 like '%" + $("#txt_jfwh").val().toUpperCase() + "%'" +
                " or 解封文号 like '%" + $("#txt_jfwh").val().toLowerCase() + "%')";
            }
            //查封 解封时间起
            if (vQLLX == "3" && !CheckObjectIsNull($("#txt_jfsjq").val())) {
                if ("@ViewBag.DBType" == "ORACLE") {
                    vFilterString += " and 解封时间>=to_date('" + $("#txt_jfsjq").val() + "','yyyy-mm-dd') ";
                } else {
                    vFilterString += " and 解封时间>='" + $("#txt_jfsjq").val() + "' ";
                }
            }
            //查封 解封时间止
            if (vQLLX == "3" && !CheckObjectIsNull($("#txt_jfsjz").val())) {
                if ("@ViewBag.DBType" == "ORACLE") {
                    vFilterString += " and 解封时间<=to_date('" + $("#txt_jfsjz").val() + "','yyyy-mm-dd') ";
                } else {
                    vFilterString += " and 解封时间<='" + $("#txt_jfsjz").val() + "' ";
                }
