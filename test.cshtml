            }
            return false;
        }

        //查看详细
        function caseView(item) {
            var vCaseNo = "";
            var vSql = "";
            vCaseNo = $(item).attr("nodeid");
            var url = "";
            var vtype = "0";
            $.ajax({
                async: false,
                type: "POST",
                url: "../Public/GetSLBH?sCaseNo=" + vCaseNo,
                datatype: "text",
                success: function (res) {
                    if (vCaseNo == res) {
                        vtype = "1";
                        url = "RealEstate/Public/CaseListInfo?CaseNo=" + vCaseNo;
                    } else {
                        vCaseNo = res;
                        url = "Case/CaseListInfo?CaseNo=" + vCaseNo;
                    }
                }
            });
            if (window.top.sysCreateTab) {
                if (vtype == "1") {
                    window.top.sysCreateTab("caseListInfo", "RealEstate/Public/CaseListInfo", "案卷办理过程", url);
                } else {
                    if ("@Request.QueryString["IsEdit"]" == "1") {
                        $.post("../Public/SaveEditHistory?strCaseNo=" + vCaseNo, function (res) {
                            if (res) {
                                window.top.sysCreateTab("caseListInfo", "Case/CaseListInfo?IsEdit=1", "案卷办理过程", url + "&IsEdit=1");
                            }
                        });
                    }
                    else {
                        window.top.sysCreateTab("caseListInfo", "Case/CaseListInfo", "案卷办理过程", url);
                    }
                }
            }
        }

