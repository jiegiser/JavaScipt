            if (filter.indexOf('异议申请人') != -1) {
                return true;
            }
            if (filter.indexOf('预告义务人') != -1) {
                return true;
            }
            if (filter.indexOf('预告权利人') != -1) {
                return true;
            }
            if (filter.indexOf('抵押权人') != -1) {
                return true;
            }
            if (filter.indexOf('抵押人') != -1) {
                return true;
            }
            if (filter.indexOf('权利人') != -1) {
